<configuration>

  <connectionStrings>
    <clear />
    <add name="mssql" connectionString="Data Source=(local);Initial Catalog=BMERP;Integrated Security=True"
    providerName="System.Data.SqlClient"/>
   

  </connectionStrings>
  <system.web>



    <compilation debug="true" defaultLanguage="c#" targetFramework="4.0">
      <assemblies>
        <add assembly="System.Design, Version=4.0.0.0, Culture=neutral, PublicKeyToken=B03F5F7F11D50A3A" />
        <add assembly="System.Web.Extensions.Design, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35" />
        <add assembly="System.Windows.Forms, Version=4.0.0.0, Culture=neutral, PublicKeyToken=B77A5C561934E089" />
      </assemblies>
    </compilation>
    <sessionState mode="InProc" timeout="120" />
    <authentication mode="Forms">
      <!--<forms loginUrl="~/forms/zlogin.aspx" protection="All" slidingExpiration="true" timeout="2880"/>-->

      <forms loginUrl="~/forms/zlogin.aspx" protection="All" cookieless="UseUri" />
    </authentication>

    <!--<authentication mode="Forms">
      <forms name=".FORM" loginUrl="~/Account/Login.aspx" defaultUrl="Default.aspx" protection="All" timeout="30" path="/">
      </forms>
    </authentication>-->

    <httpModules>
      <add name="ScriptModule" type="System.Web.Handlers.ScriptModule, System.Web.Extensions, Version=3.5.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35" />
    </httpModules>

  </system.web>
  <system.webServer>
    <modules runAllManagedModulesForAllRequests="true" />
    <validation validateIntegratedModeConfiguration="false"/>
  </system.webServer>
</configuration>
